def searchLast(arr,n,x):
    low = 0
    high = n - 1
    mid = (low+high)//2
    while low <=high:
       
        if x >arr[mid]:
            low = mid + 1
        elif x < arr[mid]:
            high = mid - 1
        else:
            if mid == n -1 or arr[mid]!=arr[mid+1]:
                return mid
            else:
                low = mid + 1
     return -1
     
#O(1) aux space
#O(log N ) time
