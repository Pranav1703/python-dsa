def searchFirst(arr,x,low,high):
    if low > high:
        return -1
    mid = (low+high)//2
    if x == arr[mid]:
        return mid 
    if x > arr[mid]:
        retrun searchFirst(arr,x,mid + 1,high)
    elif x < ar[mid]:
        return searchFirst(arr,x,low,mid - 1)
    else:
        if mid == 0 or arr[mid-1]!=arr[mid]:
            return mid
        else:
        return searchFirst(arr,x,low,mid - 1)
        
        
        
    #O(log N) aux space

       
