# iterative approch
def binarySearch(l,x):
    low = 0
    high = len(l) - 1
    
    while low <=high:
        mid = (low+high)//2
        if x == l[mid]:
            return mid
        elif x >l[mid]:
            low = mid + 1
        else
            high = mid - 1
    return -1  #if array has no elements
